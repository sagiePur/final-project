<Window x:Class="WpfApp1.AddVehicleAssign"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="AddVehicleAssign" Height="410" Width="300">
    <Grid>
        <Label Content="Vehicle" HorizontalAlignment="Left" Margin="61,18,0,0" VerticalAlignment="Top" FontSize="10"/>
        <ComboBox x:Name="VehicleComboBox" HorizontalAlignment="Left" Height="22" Margin="61,42,0,0" VerticalAlignment="Top" Width="148"/>
        <Label Content="Start Date" HorizontalAlignment="Left" Margin="61,74,0,0" VerticalAlignment="Top" FontSize="10"/>
        <xctk:DateTimePicker x:Name="StartDate" HorizontalAlignment="Left" Margin="61,102,0,0" VerticalAlignment="Top" Width="148"/>
        <Label Content="End Date" HorizontalAlignment="Left" Margin="61,141,0,0" VerticalAlignment="Top" FontSize="10"/>
        <xctk:DateTimePicker x:Name="EndDate" HorizontalAlignment="Left" Margin="61,169,0,0" VerticalAlignment="Top" Width="148"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="98,300,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
