<Window x:Class="WpfApp1.AddVehicle"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="AddVehicle" Height="410" Width="300">
    <Grid>
        <Label Content="Car Model" HorizontalAlignment="Left" Margin="61,43,0,0" VerticalAlignment="Top" FontSize="10"/>
        <TextBox x:Name="CarModelTextBox" HorizontalAlignment="Left" Height="22" Margin="61,67,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="148"/>
        <Label Content="Number Of Sits" HorizontalAlignment="Left" Margin="61,101,0,0" VerticalAlignment="Top" FontSize="10"/>
        <TextBox x:Name="SitsTextBox" HorizontalAlignment="Left" Height="22" Margin="61,125,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="148"/>
        <Label Content="Car Number" HorizontalAlignment="Left" Margin="61,163,0,0" VerticalAlignment="Top" FontSize="10"/>
        <TextBox x:Name="CarNumberTextBox" HorizontalAlignment="Left" Height="22" Margin="61,191,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="148"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="95,300,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>

    </Grid>
</Window>
